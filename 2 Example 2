void DFS(int key){
  bool checked[key] = false;
  Vector<point<int, int> > pathUsed;
  for(int i = 0; i < key; i++){
    visited[i] = false;
   }
   
   DFSHelper(0, key, checked, pathUsed, -1, 0);
}

